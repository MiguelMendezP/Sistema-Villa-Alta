<div id="containerForm" class="card mx-basic my-basic hide">
    <form id="formSalida" action="/admin/salida" method="post" enctype="multipart/form-data">
        <div class="row">
            <div class="input-field col s3">
                <label>Fecha Inicial</label>
            </div>
            <div class="input-field col s3">
                <label>Fecha Final</label>
            </div>
            <div class="input-field col s3">
                <label>Hora Formato 12H</label>
            </div>
        </div>
        <div class="row">   
            <div class="input-field col s3">
                <input id="fechaInicial" type="date" name="fecha"></input>
            </div>   
            <div class="input-field col s3">
                <input id="cantidadRepetir" type="number" min="1" max="30" step="1" value="1"> 
            </div>        
            <div class="input-field col s3">
                <input id="hora" type="time" name="hora"></input>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s4">
                <label>Terminal Origen</label>
            </div>
            <div class="input-field col s4">
                <label>Destino</label>
            </div>
        </div>
        <div class="row">
            <div class="input-field col s4">
                <select id="ciudadOrigen" class="browser-default" name="terminal_salida"></select>
            </div>
            <div class="input-field col s4">
                <select id="ciudadDestino" class="browser-default" name="terminal_destino"></select>
            </div>
        </div>

    </form>
    <div class="row">
        <div class="col s1 offset-s10">
            <button type="submit" id="btnGuardar" class="waves-effect waves-light btn my-basic mx-basic blue">Registrar</button>
        </div>
        <div class="col s1 offset-s10">
            <button id="btnCancelar" class="waves-effect waves-light btn my-basic mx-basic red">Cancelar</button>
        </div>
    </div>
</div>